<MudDialog Style="width:400px;">
    <DialogContent>
        <MudItem xs="8" md="12" sm="12">
            <center>
                <MudChip Color="Color.Warning">Bed 22</MudChip>
                <MudChip Color="Color.Secondary">Ward 3</MudChip>
            </center>
        </MudItem>
        <MudItem xs="8" md="12" sm="12">
            <center>
                <MudChip Color="Color.Info">18 May</MudChip>
                <MudChip Color="Color.Info">21:34</MudChip>
            </center>
        </MudItem>
        <MudItem xs="8" md="12" sm="12">
            <center>
                <MudChip Color="Color.Success">Not Blacklisted</MudChip>
            </center>
        </MudItem>
    </DialogContent>
    <DialogActions>
        <MudButton Style="border-radius:10px 10px 10px 10px"
                   Variant="Variant.Outlined"
                   Color="Color.Error"
                   OnClick="Cancel">Cancel</MudButton>
    </DialogActions>
</MudDialog>


@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();

    public string value;
}